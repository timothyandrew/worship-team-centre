<table class="table table-striped table-hover table-responsive">
  <tr>
    <th>Song Name</th>
    <th>Lyrics</th>
    <th>Key</th>
    <th>Added On</th>
  </tr>

  <% @songs.each do |song| %>
  <tr>
    <td><%= link_to song.name, song_path(song) %></td>
    <td><%= simple_format song.lyrics %></td>
    <td><%= song.key %></td>
    <td><%=  time_ago_in_words(song.created_at) %> ago</td>
  </tr>
  <% end %>
</table>